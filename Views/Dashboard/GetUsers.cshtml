@model Test_Owin_Identity.Models.ApplicationUser
@{
    ViewBag.Title = "GetUsers";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>GetUsers</h2>
<div class="col-md-12">
    @using (Html.BeginForm("GetUsers", "Dashboard", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
    {
       @Html.AntiForgeryToken()


        <div class="form-group">
           @* <div class="col-md-4"></div>*@

            <lable class="col-md-3">
                @Html.Label("User List", new { @class = "control-label" })
                </lable>
                <div class="col-md-10">

                    @Html.DropDownListFor(m => m.UserName, (List<SelectListItem>)ViewBag.UserName);
                </div>
            </div>
        
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" class="btn btn-default" value="Assign" />
            </div>
        </div>

    }
</div>